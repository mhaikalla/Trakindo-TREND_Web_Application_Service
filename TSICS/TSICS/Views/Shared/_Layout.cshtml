<body class="bg-light">
    <div class="allwrap">
        <header class="header bg-dark position-sticky shadow-sm text-white p-0">
            <div class="row bg-yellow-grad pt-1 pt-sm-2 pt-md-3 pt-lg-4"></div>
            <div class="container">
                @if (!Request.RawUrl.ToString().Contains("/TechnicalRequest/GenerateCatElectronicTechniciann/"))
                {
                <div class="row flex-fill">
                    <div class="col-12 col-lg flex-grow-0 d-flex justify-content-between justify-content-lg-start">
                        <a href="@Url.Action("Index", "Home")" class="logo-main d-flex align-items-center justify-content-center my-2">
                            <picture class="d-flex"><img src="~/assets/images/repository/tsics-logo.jpg" alt="" style=" height: 43px;"></picture>
                        </a><a id="toggle-menu" href="#" class="btn rounded-0 btn-toggle-menu d-block d-lg-none h-100 px-3"><span class="box"></span><span class="inner"></span></a>
                    </div>
                    @if (Request.RawUrl.ToString().ToLower() == "/account/login")
                    {
                        <div class="col-lg flex-grow-1 d-lg-flex d-none navbar-main">
                            <div class="col d-flex align-items-center px-0">
                                    <h1 class="h4 font-secondary m-0">TECHNICAL SUPPORT INFORMATION &amp; COMMUNICATION SYSTEM</h1>
                            </div>
                        </div>
                    }
                    else if (Request.RawUrl.ToString().ToLower() == "/account/register")
                    {

                        <div class="col-lg flex-grow-1 d-lg-flex d-none navbar-main justify-content-between">
                            <ul class="nav font-secondary">
                                <li data-active_nav="library" class="nav-item h-100">
                                    @Html.ActionLink("LIBRARY", "Index", "Literation", new { area = "" }, new { @class = "h4 nav-link d-flex h-100 align-items-center mr-3 mb-0 px-3" })
                                </li>
                                <li data-active_nav="register" class="nav-item h-100">
                                    @Html.ActionLink("REGISTER", "Register", "Account", new { area = "" }, new { @class = "h4 nav-link d-flex h-100 align-items-center mr-3 mb-0 px-3" })
                                </li>
                            </ul>
                        </div>
                    }
                    else
                    {
                        <div class="col-lg flex-grow-1 d-lg-flex d-none navbar-main justify-content-between">
                            <ul class="nav font-secondary">
                                <li data-active_nav="library" class="nav-item h-100">
                                    @Html.ActionLink("LIBRARY", "Index", "Library", new { area = "" }, new { @class = "h4 nav-link d-flex h-100 align-items-center mr-3 mb-0 px-3" })
                                </li>
                                @if (Session["userid"] != null)
                                {
                                    if (Session["roleColor"] != "Guest")
                                    {
                                        <li data-active_nav="the-technical-request" class="nav-item h-100">
                                            @Html.ActionLink("TECHNICAL REQUEST", "Index", "TechnicalRequest", new { area = "" }, new { @class = "h4 nav-link d-flex h-100 align-items-center mr-3 mb-0 px-3" })
                                        </li>
                                    }

                                }
                                <li data-active_nav="dashboard" class="nav-item h-100"> 
                                    @Html.ActionLink("DASHBOARD", "Index", "Dashboard", new { area = "" }, new { @class = "h4 nav-link d-flex h-100 align-items-center mr-3 mb-0 px-3" })
                                </li>
                            </ul>
                            <div class="profile-pict d-flex align-items-center position-relative">
                                <div class="media align-items-center h-100">
                                    @Html.Partial("_LoginPartial")
                                </div>
                            </div>
                        </div>
                    }                    
                </div>
                }
                else
                {
                    <div class="col-12 col-lg flex-grow-0 d-flex justify-content-between justify-content-lg-start">
                        <a class="logo-main d-flex align-items-center justify-content-center my-2">
