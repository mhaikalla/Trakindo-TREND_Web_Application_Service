                                                                    <label class="custom-checkbox form-check mr-4">
                                                                        <input id="cbPriority" type="checkbox" name="pipPriority" checked class="form-check-input">
                                                                        <div class="form-check-label custom-label"><i class="icon fa fa-check"></i><span class="text">PIP Priority</span></div>
                                                                    </label>
                                                                }
                                                                else
                                                                {
                                                                    <label class="custom-checkbox form-check mr-4">
                                                                        <input id="cbPriority" type="checkbox" name="pipPriority" class="form-check-input">
                                                                        <div class="form-check-label custom-label"><i class="icon fa fa-check"></i><span class="text">PIP Priority</span></div>
                                                                    </label>
                                                                }
                                                                @if (getDataFC.PspProactive == "on")
