                                                                    <select data-title="Choose fields" data-style="border bg-white rounded-0" data-width="100%" data-size="5" name="priority[]" multiple disabled id="select-filter-8">
                                                                        <option value="0">Choose fields</option>
                                                                    </select>
                                                                </div>

                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        @if (getDataFC.ChecklistArea == "on" || getDataFC.ChecklistModel == "on" || getDataFC.ChecklistPriority == "on" || getDataFC.ChecklistPSLNo == "on" || getDataFC.ChecklistPSLStatus == "on" || getDataFC.ChecklistSalesOffice == "on" || getDataFC.ChecklistSerialNumber == "on" || getDataFC.ChecklistAgeIndicator == "on")
                                                        {
                                                            <p>Filter Result : </p>
                                                        }
                                                        
                                                        <div class="col-md-12">
                                                            @if (getDataFC.ChecklistArea == "on")
                                                            {
                                                                <div class="form-group mr-3">
                                                                    <p>Area : @getDataFC.Area</p>
                                                                </div>
                                                            }
                                                            @if (getDataFC.ChecklistSalesOffice == "on")
                                                            {
                                                                <div class="form-group mr-3">
                                                                    <p>Sales Office : @getDataFC.SalesOffice</p>
                                                                </div>
                                                            }
                                                            @if (getDataFC.ChecklistPSLStatus == "on")
