                                                                        <input id="cbProActive" type="checkbox" name="pspProactive" checked class="form-check-input">
                                                                        <div class="form-check-label custom-label"><i class="icon fa fa-check"></i><span class="text">PSP Pro-Active</span></div>
                                                                    </label>
                                                                }
                                                                else
                                                                {
                                                                    <label class="custom-checkbox form-check mr-4">
                                                                        <input id="cbProActive" type="checkbox" name="pspProactive" class="form-check-input">
                                                                        <div class="form-check-label custom-label"><i class="icon fa fa-check"></i><span class="text">PSP Pro-Active</span></div>
                                                                    </label>
                                                                }
