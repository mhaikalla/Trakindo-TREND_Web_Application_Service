                                                                <div class="form-group mr-3">
                                                                    <p>PSL Status : @getDataFC.PSLStatus</p>
                                                                </div>
                                                            }
                                                            @if (getDataFC.ChecklistPSLNo == "on")
                                                            {
                                                                <div class="form-group mr-3">
                                                                    <p>PSL No : @getDataFC.PSLNo</p>
                                                                </div>
                                                            }
                                                            @if (getDataFC.ChecklistAgeIndicator == "on")
                                                            {
                                                                <div class="form-group mr-3">
                                                                    <p>Age Indicator : @getDataFC.AgeIndicator</p>
                                                                </div>
                                                            }
                                                            @if (getDataFC.ChecklistSerialNumber == "on")
                                                            {
                                                                <div class="form-group mr-3">
                                                                    <p>Serial Number : @getDataFC.SerialNumber</p>
                                                                </div>
                                                            }
                                                            @if (getDataFC.ChecklistModel == "on")
                                                            {
                                                                <div class="form-group mr-3">
                                                                    <p>Model : @getDataFC.Model</p>
                                                                </div>
                                                            }
                                                            @if (getDataFC.ChecklistPriority == "on")
                                                            {
                                                                <div class="form-group mr-3">
                                                                    <p>Priority : @getDataFC.Priority</p>
                                                                </div>
                                                            }
                                                        </div>
                                                    </div>
                                                   
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="text-center">
                                                                <button type="submit" class="btn btn-warning btn-show-result px-4 py-2">Show Result</button>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </fieldset>
                                        </div>
                                    </div>
                                </div>
                            </section>
                            <section>
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="card bg-white shadow-sm border-0 mb-3">
                                            <div id="total_outstanding_psl" class="card-body">
                                                <div class="d-sm-flex justify-content-between mb-4">
                                                    <div class="title mb-3 mb-sm-0">
                                                        <div class="h6 mb-0">Total Outstanding PSL</div>
                                                        <div class="small text-muted">Total <span id='total-data-psl'>-</span> Outstanding PSL</div>
                                                    </div>
                                                    <div>
                                                        <a href="javascript:;" id="btn-chart-back" style="display: none;" class="btn btn-primary btn-sm align-self-baseline"><i class="fa fa-redo mr-2"></i><span>Reset</span></a>
                                                        <button data-img-target="#total_outstanding_psl" data-img-filename="total_outstanding_psl" type="button" class="btn-img-download btn btn-outline-secondary btn-sm ml-3"><small>Save as image</small></button>
                                                    </div>
                                                </div>
                                                <div class="table-responsive">
                                                    <div class="chart vertical-bar">
