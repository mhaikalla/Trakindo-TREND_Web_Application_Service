@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Views/Shared/_LayoutDashboard.cshtml";
    var getDataFC = (GetFormCollectionPSLOutstanding)ViewBag.ModelGetFormCollection;
    var urlget = "?PipSafety=" + getDataFC.PipSafety + "&PipPriority=" + getDataFC.PipPriority + "&PspProactive=" + getDataFC.PspProactive + "&PspAfterFailure=" + getDataFC.PspAfterFailure + "&ReleaseDateFrom=" + getDataFC.ReleaseDateFrom + "&ReleaseDateEnd=" + getDataFC.ReleaseDateEnd + "&TerminationDateFrom=" + getDataFC.TerminationDateFrom + "&TerminationDateEnd=" + getDataFC.TerminationDateEnd + "&Inventory=" + getDataFC.Inventory + "&Rental=" + getDataFC.Rental + "&HID=" + getDataFC.HID + "&Areass=" + getDataFC.Area + "&SalesOffice=" + getDataFC.SalesOffice + "&PSLStatus=" + getDataFC.PSLStatus + "&PSLNo=" + getDataFC.PSLNo + "&AgeIndicator=" + getDataFC.AgeIndicator + "&SerialNumber=" + getDataFC.SerialNumber + "&Model=" + getDataFC.Model + "&Priority=" + getDataFC.Priority + "&checklistArea=" + getDataFC.ChecklistArea + "&checklistSalesOffice=" + getDataFC.ChecklistSalesOffice + "&checklistPSLStatus=" + getDataFC.ChecklistPSLStatus + "&checklistPSLNo=" + getDataFC.ChecklistPSLNo + "&checklistAgeIndicator=" + getDataFC.ChecklistAgeIndicator + "&checklistSerialNumber=" + getDataFC.ChecklistSerialNumber + "&checklistModel=" + getDataFC.ChecklistModel + "&checklistPriority=" + getDataFC.Priority + "&others=" + getDataFC.Others;
