                                                                <label class="custom-checkbox form-check">
                                                                    <input id="cb4" type="checkbox" data-checkbox-toggle-disable="{'target': '#select-filter-4'}" class="form-check-input">
                                                                    <div class="form-check-label custom-label"><i class="icon fa fa-check"></i><span class="text">PSL Status</span></div>
                                                                </label>
                                                                <div class="input-group">
                                                                    <select data-title="Choose fields" data-style="border bg-white rounded-0" data-width="100%" data-size="5" name="pslStatus[]" multiple disabled id="select-filter-4">
                                                                        <option value="0">Choose fields</option>
                                                                    </select>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-3">
                                                            <div class="form-group mr-3">
                                                                <label class="custom-checkbox form-check">
                                                                    <input id="cb5" type="checkbox" data-checkbox-toggle-disable="{'target': '#select-filter-5'}" class="form-check-input">
                                                                    <div class="form-check-label custom-label"><i class="icon fa fa-check"></i><span class="text">SO Status</span></div>
                                                                </label>
                                                                <div class="input-group">
                                                                    <select data-title="Choose fie  lds" data-style="border bg-white rounded-0" data-width="100%" data-size="5" name="soStatus[]" multiple disabled id="select-filter-5">
                                                                        <option value="0">Choose fields</option>
                                                                    </select>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row mb-3">
                                                        <div class="col-md-12">
                                                            <label>Choose optional values to be shown</label>
                                                            <div class="form-group form-inline">
                                                                @if (getDataFC.SRNo == "on")
                                                                {
                                                                    <label class="custom-checkbox form-check mr-4">
                                                                        <input type="checkbox" checked name="SRNo" class="form-check-input">
                                                                        <div class="form-check-label custom-label"><i class="icon fa fa-check"></i><span class="text">SR No</span></div>
                                                                    </label>
                                                                }
                                                                else
                                                                {
                                                                    <label class="custom-checkbox form-check mr-4">
                                                                        <input type="checkbox" name="SRNo" class="form-check-input">
                                                                        <div class="form-check-label custom-label"><i class="icon fa fa-check"></i><span class="text">SR No</span></div>
                                                                    </label>
                                                                }

                                                                @if (getDataFC.QuotationNo == "on")
                                                                {
                                                                    <label class="custom-checkbox form-check mr-4">
                                                                        <input type="checkbox" checked name="QuotNo" class="form-check-input">
                                                                        <div class="form-check-label custom-label"><i class="icon fa fa-check"></i><span class="text">Quotation No</span></div>
                                                                    </label>
                                                                }
                                                                else
                                                                {
                                                                    <label class="custom-checkbox form-check mr-4">
                                                                        <input type="checkbox" name="QuotNo" class="form-check-input">
                                                                        <div class="form-check-label custom-label"><i class="icon fa fa-check"></i><span class="text">Quotation No</span></div>
                                                                    </label>
                                                                }

                                                                @if (getDataFC.SONo == "on")
                                                                {
                                                                    <label class="custom-checkbox form-check mr-4">
                                                                        <input type="checkbox" checked name="SONo" class="form-check-input">
                                                                        <div class="form-check-label custom-label"><i class="icon fa fa-check"></i><span class="text">SO No</span></div>
                                                                    </label>
                                                                }
                                                                else
                                                                {
                                                                    <label class="custom-checkbox form-check mr-4">
                                                                        <input type="checkbox" name="SONo" class="form-check-input">
                                                                        <div class="form-check-label custom-label"><i class="icon fa fa-check"></i><span class="text">SO No</span></div>
                                                                    </label>
                                                                }

                                                                @if (getDataFC.ChecklistPSLStatus == "on")
                                                                {
                                                                    <label class="custom-checkbox form-check mr-4">
                                                                        <input type="checkbox" checked name="checklistPSLStatus" class="form-check-input">
                                                                        <div class="form-check-label custom-label"><i class="icon fa fa-check"></i><span class="text">PSL Status</span></div>
                                                                    </label>
                                                                }
                                                                else
                                                                {
                                                                    <label class="custom-checkbox form-check mr-4">
                                                                        <input type="checkbox" name="checklistPSLStatus" class="form-check-input">
                                                                        <div class="form-check-label custom-label"><i class="icon fa fa-check"></i><span class="text">PSL Status</span></div>
                                                                    </label>
                                                                }

                                                                @if (getDataFC.ChecklistSOStatus == "on")
                                                                {
                                                                    <label class="custom-checkbox form-check mr-4">
                                                                        <input type="checkbox" checked name="checklistSOStatus" class="form-check-input">
                                                                        <div class="form-check-label custom-label"><i class="icon fa fa-check"></i><span class="text">SO Status</span></div>
                                                                    </label>
                                                                }
                                                                else
                                                                {
                                                                    <label class="custom-checkbox form-check mr-4">
                                                                        <input type="checkbox" name="checklistSOStatus" class="form-check-input">
                                                                        <div class="form-check-label custom-label"><i class="icon fa fa-check"></i><span class="text">SO Status</span></div>
                                                                    </label>
                                                                }


                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        @if (getDataFC.Area != "" || getDataFC.Model != "" || getDataFC.PSLNo != "" || getDataFC.PSLStatus != "" || getDataFC.SOStatus != "")
                                                        {
                                                            <p>Filter :</p>
                                                        }
                                                        <div class="col-md-12">
                                                            @if (getDataFC.Area != "")
                                                            {
                                                                <div class="form-group mr-3">
                                                                    <p>Area : @getDataFC.Area</p>
                                                                </div>
                                                            }
                                                            @if (getDataFC.Model != "")
                                                            {
                                                                <div class="form-group mr-3">
                                                                    <p>Model : @getDataFC.Model</p>
                                                                </div>
                                                            }
                                                            @if (getDataFC.PSLStatus != "")
                                                            {
                                                                <div class="form-group mr-3">
                                                                    <p>PSL Status : @getDataFC.PSLStatus</p>
                                                                </div>
                                                            }
                                                            @if (getDataFC.PSLNo != "")
                                                            {
                                                                <div class="form-group mr-3">
                                                                    <p>PSL No : @getDataFC.PSLNo</p>
                                                                </div>
                                                            }
                                                            @if (getDataFC.SOStatus != "")
                                                            {
                                                                <div class="form-group mr-3">
                                                                    <p>SO Status : @getDataFC.SOStatus</p>
                                                                </div>
                                                            }
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="text-center">
                                                                <button type="submit" class="btn btn-warning btn-show-result px-4 py-2">Show Result</button>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </fieldset>
                                        </div>
                                    </div>
                                </div>
                            </section>
                            <section>
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="card bg-white shadow-sm border-0 mb-3">
                                            <div class="card-body">
                                                <div class="border p-3">
                                                    <div id="table--psl-execution" data-dt-api="@Url.Action("TableDashboardPSLExecution","Dashboard")@urlGet" class="table-responsive">
                                                        <table class="table table-dt small table-align-top">
                                                            <thead>
                                                                <tr class="text-center">
                                                                    <th><span class="text-nowrap">Serial No</span></th>
                                                                    <th><span class="text-nowrap">Model</span></th>
                                                                    <th><span class="text-nowrap">Area</span></th>
                                                                    <th><span class="text-nowrap">Sales Office</span></th>
                                                                    <th><span class="text-nowrap">Customer</span></th>
                                                                    <th><span class="text-nowrap">Status</span></th>
                                                                </tr>
                                                            </thead>
                                                        </table>
                                                        <script>
                                    var htmlDescList = function (resp) {
                                      var html = '<dl class="row no-gutters m-0" style="width: 180px;">'

                                    resp.forEach((el, i) => {
                                        html += (el.divider && i > 0) ? '<dt class="col-6"><hr class="my-2"/></dt><dd class="col-6"><hr class="my-2"/></dd>' : ''
                                        html += '<dt class="col-6">' + el.title + '</dt>'
                                        html += '<dd class="col-6"> : ' + el.value + '</dd>'
                                    })

                                      html += '</dl>'

                                      return html
                                    }
                                    var column_dt_table = [
                                      {
                                        data    : 'serial_no',
                                        render  : function (data,type,row) {
                                          var html = ''
                                          html = '<span class="' + data.style + '">' + data.value + '</span>'
                                          return html
                                        }
                                        },
                                        {
                                            data: 'model',
                                            render: function (data, type, row) {
                                                var html = ''
                                                html = '<span class="' + data.style + '">' + data.value + '</span>'
                                                return html
                                            }
                                        },
                                      {
                                        data    : 'area',
                                        render  : function (data,type,row) {
                                          var html = ''
                                          html = '<span class="' + data.style + '">' + data.value + '</span>'
                                          return html
                                        }
                                      },
                                      {
                                        data    : 'sales_office',
                                        render  : function (data,type,row) {
                                          var html = ''
                                          html = '<span class="' + data.style + '">' + data.value + '</span>'
                                          return html
                                        }
                                      },
                                      {
                                        data    : 'customer',
                                        render  : function (data,type,row) {
                                          var html = ''
                                          html = '<span class="' + data.style + '">' + data.value + '</span>'
                                          return html
                                        }
                                      },
                                      {
                                        data    : 'ls44043',
                                        render  : function (data,type,row) {
                                          var html = ''
                                          html = htmlDescList(data.val_arr)
                                          return html
                                        }
                                      },
                                    ]
                                                        </script>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </section>
                        </form>
                    </div>
                </div>
            </div>
        </section>
    </div>
</main>
@section ScriptBottom {
    <script type="text/javascript">
    $(document).ready(function () {
        var inventory = "";
        var rental = "";
        var hid = "";
        var others = "";
        $("#cbinventory").on('click', function () {
            var that = $(this);
            if (that.prop("checked") == true) {
                inventory = "on";
            } else {
                inventory = "";
            }
        });
        $("#cbrental").on('click', function () {
            var that = $(this);
            if (that.prop("checked") == true) {
                rental = "on";
            } else {
                rental = "";
            }
        });
        $("#cbhid").on('click', function () {
            var that = $(this);
