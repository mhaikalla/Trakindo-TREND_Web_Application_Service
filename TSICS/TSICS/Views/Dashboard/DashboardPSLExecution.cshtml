@using Com.Trakindo.TSICS.Data.Model
@{
    ViewBag.Title = "Product Health";
    Layout = "~/Views/Shared/_LayoutDashboard.cshtml";
    var getDataFC = (GetFormCollectionPSLExecution)ViewBag.ModelGetFormCollectionPSLExecution;
    var urlGet = "?Inventory=" + getDataFC.Inventory + "&Rental=" + getDataFC.Rental + "&HID=" + getDataFC.HID + "&Others=" + getDataFC.Others + "&Areas=" + getDataFC.Area + "&SalesOffice=" + getDataFC.SalesOffice + "&PSLStatus=" + getDataFC.PSLStatus + "&PSLNo=" + getDataFC.PSLNo + "&AgeIndicator=" + getDataFC.AgeIndicator + "&SerialNumber=" + getDataFC.SerialNumber + "&Model=" + getDataFC.Model + "&Priority=" + getDataFC.Priority + "&SRNo=" + getDataFC.SRNo + "&QuotNo=" + getDataFC.QuotationNo + "&SONo=" + getDataFC.SONo + "&SOStatus=" + getDataFC.SOStatus + "&checklistPSLStatus=" + getDataFC.ChecklistPSLStatus + "&checklistSOStatus=" + getDataFC.ChecklistSOStatus;
